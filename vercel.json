{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/node"
    }
  ],
  "buildCommand": "cd client && npm run build",
  "devCommand": "cd client && npm run start",
  "installCommand": "cd client && npm install",
  "outputDirectory": "client/build",
  "env": {
    "NODE_ENV": "production"
  }
}
